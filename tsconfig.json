{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "removeComments": true,
        "strictNullChecks": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "baseUrl": "./",
        "types": [
            "node",
            "jest"
        ]
    }
}